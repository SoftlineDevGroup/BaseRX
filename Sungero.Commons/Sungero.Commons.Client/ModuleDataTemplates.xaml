    <DataTemplate DataType="{x:Type local:ExternalEntityLinkCollectionPresenter`1}">
      <local:ExternalEntityLinkCollectionView />
    </DataTemplate>
      <DataTemplate DataType="{x:Type local:ExternalEntityLinkPresenter`1}">
        <local:ExternalEntityLinkView />
