<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:local="clr-namespace:Sungero.Meetings.Client">

    <DataTemplate DataType="{x:Type local:AgendaCollectionPresenter`1}">
      <local:AgendaCollectionView />
    </DataTemplate>
      <DataTemplate DataType="{x:Type local:AgendaPresenter`1}">
        <local:AgendaView />
      </DataTemplate>

    <DataTemplate DataType="{x:Type local:MeetingCollectionPresenter`1}">
      <local:MeetingCollectionView />
    </DataTemplate>
      <DataTemplate DataType="{x:Type local:MeetingPresenter`1}">
        <local:MeetingView />
      </DataTemplate>

    <DataTemplate DataType="{x:Type local:MinutesCollectionPresenter`1}">
      <local:MinutesCollectionView />
    </DataTemplate>
      <DataTemplate DataType="{x:Type local:MinutesPresenter`1}">
        <local:MinutesView />
      </DataTemplate>


</ResourceDictionary>